	gridstab.f cice.f rwalsh.f lwalsh.f intpol2.f

INC = dimension.h


CF =            xlf90_r
FFLAGS =  -q64 -O2 -qrealsize=8  -qextname -qarch=auto -qtune=auto\
		  -qcache=auto -qmaxmem=-1 -qnosave

LINKFLAGS = -q64 -O2 -qrealsize=8  -qextname -qarch=auto -qtune=auto -qcache=auto \
			-qmaxmem=-1 -qnosave


.f.o:
	$(CF)  -c  -qfixed $(FFLAGS) -o $*.o $*.f


newf:	$(OBJS) 
	$(CF)  -o newf $(LINKFLAGS)  $(OBJS)

$(OBJS):  $(INC)
