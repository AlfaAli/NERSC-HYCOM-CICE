	rHeRo.o recmw2.o recmwv.o rgrsp.o rlatlon.o rmask.o  \
	rotate.o tecall.o spherdist.o dmpwin.o rcoads.o \
	recmwf.o lamip.o mixairt.o lcoads3.o lecmw3.o rstat.o \
	fillall.o tec1.o tecij.o mercator.o cdepth.o \
	filldepth.o shap2d.o dmp2dtec.o crain.o lrain.o rrain.o \
	gridstab.o cice.o rwalsh.o lwalsh.o intpol2.o

SRC =	cairt.f ccloud.f chumid.f csst.f cuv.f cwind.f cwstde.f\
	dmpdat.f fillup.f inclcc.f intpol.f lcoads.f lecmw2.f\
